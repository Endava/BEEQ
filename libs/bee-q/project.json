{
  "name": "bee-q",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/bee-q/src",
  "projectType": "library",
  "tags": [],
  "generators": {
    "@nxext/stencil:component": {
      "style": "scss"
    }
  },
  "targets": {
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["nx run bee-q:stencil-start", "nx run bee-q:storybook-css-start", "nx run bee-q:storybook-start"],
        "parallel": true
      }
    },
    "build": {
      "dependsOn": ["^build"],
      "executor": "@nxext/stencil:build",
      "outputs": ["{options.outputPath}", "libs/bee-q"],
      "options": {
        "projectType": "library",
        "tsConfig": "libs/bee-q/tsconfig.lib.json",
        "configPath": "libs/bee-q/stencil.config.ts",
        "outputPath": "dist/bee-q",
        "ci": true
      },
      "configurations": {
        "production": {
          "dev": false,
          "prod": true
        }
      }
    },
    "stencil-start": {
      "executor": "@nxext/stencil:serve",
      "outputs": ["{options.outputPath}"],
      "options": {
        "projectType": "library",
        "tsConfig": "libs/bee-q/tsconfig.lib.json",
        "configPath": "libs/bee-q/stencil.config.ts",
        "outputPath": "dist/bee-q"
      }
    },
    "storybook-start": {
      "executor": "@nrwl/storybook:storybook",
      "options": {
        "config": {
          "configFolder": "libs/bee-q/.storybook"
        },
        "host": "127.0.0.1",
        "port": 9001,
        "uiFramework": "@storybook/web-components"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "storybook-build": {
      "executor": "@nrwl/storybook:build",
      "outputs": ["{options.outputPath}"],
      "dependsOn": ["storybook-css-build"],
      "options": {
        "uiFramework": "@storybook/web-components",
        "outputPath": "dist/storybook/bee-q",
        "config": {
          "configFolder": "libs/bee-q/.storybook"
        },
        "webpackStatsJson": true,
        "ci": true
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "storybook-css-start": {
      "executor": "nx:run-commands",
      "outputs": ["libs/bee-q/.storybook/css"],
      "options": {
        "command": "npx tailwindcss -i libs/bee-q/src/global/styles/tailwind.pcss -o libs/bee-q/.storybook/css/styles.css -c tailwind.config.js --watch"
      }
    },
    "storybook-css-build": {
      "executor": "nx:run-commands",
      "outputs": ["libs/bee-q/.storybook/css"],
      "options": {
        "command": "npx tailwindcss -i libs/bee-q/src/global/styles/tailwind.pcss -o libs/bee-q/.storybook/css/styles.css -c tailwind.config.js --minify"
      }
    },
    "generate-icons": {
      "executor": "@bee-q/icons:generate",
      "outputs": ["{options.downloadPath}", "{options.extractToPath}", "{options.outputDir}"],
      "options": {
        "assetsFolder": "assets",
        "downloadPath": "libs/bee-q-icons/temp",
        "extractToPath": "libs/bee-q/src/components/icon/svg",
        "fileName": "master.zip",
        "helperFile": "icons-set.ts",
        "outputDir": "libs/bee-q/src/components/icon/helper",
        "sourceDir": "libs/bee-q/src/components/icon/svg/regular",
        "svgFolder": "web-master",
        "sourceUrl": "https://github.com/phosphor-icons/phosphor-icons/archive/refs/heads"
      }
    },
    "test": {
      "executor": "@nxext/stencil:test",
      "outputs": ["{options.outputPath}"],
      "options": {
        "projectType": "library",
        "tsConfig": "libs/bee-q/tsconfig.lib.json",
        "configPath": "libs/bee-q/stencil.config.ts",
        "outputPath": "dist/bee-q"
      },
      "dependsOn": ["stylelint", "lint"]
    },
    "e2e": {
      "executor": "@nxext/stencil:e2e",
      "outputs": ["{options.outputPath}"],
      "options": {
        "projectType": "library",
        "tsConfig": "libs/bee-q/tsconfig.lib.json",
        "configPath": "libs/bee-q/stencil.config.ts",
        "outputPath": "dist/bee-q",
        "noBuild": true
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["nx run bee-q:eslint", "nx run bee-q:stylelint"],
        "parallel": true
      }
    },
    "eslint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/bee-q/**/*.{ts,tsx}"]
      }
    },
    "stylelint": {
      "executor": "nx-stylelint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/bee-q/**/*.css", "libs/bee-q/**/*.scss"]
      }
    },
    "publish": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "access": "public",
        "buildTarget": "production",
        "tag": "latest"
      }
    },
    "publish-beta": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "access": "public",
        "buildTarget": "production",
        "tag": "beta"
      }
    }
  }
}
