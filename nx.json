{
  "affected": {
    "defaultBase": "main"
  },
  "cli": {
    "defaultCollection": "@nxext/stencil"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*",
    ".stylelintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": [
          "build",
          "build-storybook",
          "lint",
          "test",
          "e2e",
          "stylelint",
          "create-icons-set",
          "extract-icons"
        ]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ],
    "version": [
      {
        "target": "version",
        "projects": "dependencies"
      }
    ]
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"]
    },
    "github": {
      "dependsOn": ["build"]
    },
    "version": {
      "dependsOn": ["build"]
    }
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    }
  }
}
