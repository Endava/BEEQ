    "vite-tsconfig-paths": "4.3.1",
    "vue": "3.4.21"
  },
  "overrides": {
    "@nxext/stencil": {
      "@nx/eslint": "$@nx/eslint",
      "@nx/jest": "$@nx/jest",
      "@nx/react": "$@nx/react",
      "@nx/storybook": "$@nx/storybook"
