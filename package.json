{
  "name": "root",
  "private": true,
  "description": "Bee-Q Design System",
  "license": "Apache-2.0",
  "version": "0.0.1-beta.1",
  "repository": {
    "type": "Git",
    "url": "https://github.com/dgonzalezr/bee-q"
  },
  "scripts": {
    "start": "nx run-many --target=start --projects=bee-q",
    "build": "run-s build:libs build:storybook",
    "build:libs": "nx run-many --target=build --exclude=workspace --output-style=stream-without-prefixes",
    "build:storybook": "nx run bee-q:build-storybook --output-style=stream-without-prefixes",
    "test": "nx affected --target=test --parallel=1 --exclude=workspace",
    "clean": "run-s clean:build clean:modules",
    "clean:build": "rimraf ./dist/**",
    "clean:modules": "rimraf node_modules",
    "pre-commit": "lint-staged --verbose",
    "prepare": "husky install"
  },
  "dependencies": {
    "core-js": "^3.25.2",
    "react": "18.2.0",
