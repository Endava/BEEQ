    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "0.6.14",
    "fs-extra": "11.1.1",
    "husky": "8.0.3",
    "jest": "27.5.1",
    "jest-environment-jsdom": "27.5.1",
    "jsonc-eslint-parser": "2.3.0",
    "lint-staged": "14.0.1",
    "lit-html": "2.8.0",
    "nanospinner": "1.1.0",
    "ng-packagr": "14.2.2",
    "ngx-deploy-npm": "7.0.1",
    "npm-run-all": "4.1.5",
