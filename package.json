{
  "name": "beeq",
  "version": "0.8.2",
  "description": "BEEQ Design System",
  "license": "Apache-2.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "repository": {
    "type": "Git",
    "url": "https://github.com/Endava/BEEQ"
  },
  "scripts": {
    "prestart": "nx run beeq:icons",
    "prebuild": "npm run clean:build",
    "start": "nx run-many -t start -p beeq --skip-nx-cache",
    "build": "run-s build:*",
    "build:beeq": "nx run beeq:build --output-style=stream-without-prefixes",
    "build:wrappers": "nx run-many -t build -p tag:wrapper --output-style=stream-without-prefixes",
    "build:storybook": "nx run beeq:storybook-build --output-style=stream-without-prefixes",
    "test": "run-s test:*",
    "test:spec": "npx nx run beeq:test --output-style=stream-without-prefixes",
    "test:e2e": "npx nx run beeq:e2e --output-style=stream-without-prefixes",
    "clean": "run-s clean:build clean:modules",
    "clean:build": "rimraf dist packages/beeq/.stencil",
    "clean:modules": "rimraf node_modules .cache packages/**/node_modules",
    "pre-commit": "lint-staged --verbose",
    "prepare": "husky install",
    "postinstall": "node node_modules/puppeteer/install.mjs && ngcc --properties es2020 browser module main",
    "g": "plop --cwd tools/src/generators/plop"
  },
  "dependencies": {
    "@angular/animations": "14.3.0",
    "@angular/common": "14.3.0",
    "@angular/compiler": "14.3.0",
    "@angular/core": "14.3.0",
    "@angular/forms": "14.3.0",
    "@angular/platform-browser": "14.3.0",
    "@angular/platform-browser-dynamic": "14.3.0",
    "@angular/router": "14.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "zone.js": "0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.2.12",
    "@angular/cli": "14.2.12",
    "@angular/compiler-cli": "14.3.0",
    "@angular/language-service": "14.3.0",
    "@babel/core": "7.23.0",
    "@babel/preset-react": "7.22.15",
    "@commitlint/cli": "17.7.2",
    "@commitlint/config-angular": "17.7.0",
    "@commitlint/config-conventional": "17.7.0",
    "@jscutlery/semver": "3.1.0",
    "@nx/angular": "16.9.1",
    "@nx/devkit": "16.9.1",
    "@nx/eslint-plugin": "16.9.1",
    "@nx/jest": "16.9.1",
    "@nx/js": "16.9.1",
    "@nx/linter": "16.9.1",
    "@nx/plugin": "16.9.1",
    "@nx/react": "16.9.1",
    "@nx/rollup": "16.9.1",
    "@nx/storybook": "16.9.1",
    "@nx/workspace": "16.9.1",
    "@nxext/stencil": "16.3.0",
    "@rollup/plugin-url": "8.0.1",
    "@schematics/angular": "14.2.12",
    "@stencil/angular-output-target": "0.8.2",
    "@stencil/core": "4.4.0",
    "@stencil/react-output-target": "0.5.3",
    "@stencil/sass": "3.0.6",
    "@storybook/addon-a11y": "7.4.5",
    "@storybook/addon-actions": "7.4.5",
    "@storybook/addon-docs": "7.4.5",
    "@storybook/addon-essentials": "7.4.5",
    "@storybook/addon-links": "7.4.5",
    "@storybook/builder-vite": "7.4.5",
    "@storybook/mdx2-csf": "1.1.0",
    "@storybook/web-components": "7.4.5",
    "@storybook/web-components-vite": "7.4.5",
    "@swc-node/register": "1.6.8",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.91",
    "@swc/helpers": "0.5.2",
    "@testing-library/react": "14.0.0",
    "@types/jest": "27.5.2",
    "@types/mdx": "2.0.8",
    "@types/node": "20.8.2",
    "@types/react": "18.2.24",
    "@types/react-dom": "18.2.9",
    "@typescript-eslint/eslint-plugin": "6.7.4",
    "@typescript-eslint/parser": "6.7.4",
    "autoprefixer": "10.4.16",
    "babel-jest": "27.5.1",
    "chromatic": "7.2.2",
    "copy-webpack-plugin": "11.0.0",
    "decompress": "4.2.1",
    "eslint": "8.50.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.28.1",
