{
  "name": "bee-q",
  "version": "0.2.2",
  "description": "Bee-Q Design System",
  "license": "Apache-2.0",
  "private": true,
  "workspaces": [
    "packages/**"
  ],
  "repository": {
    "type": "Git",
    "url": "https://github.com/Endava/bee-q"
  },
  "scripts": {
    "prestart": "nx run bee-q:generate-icons",
    "start": "nx run-many --target=start --projects=bee-q --skip-nx-cache",
    "build": "run-s build:libs build:storybook:css build:storybook",
    "build:libs": "nx run-many --target=build --exclude=workspace --output-style=stream-without-prefixes",
    "build:storybook": "nx run bee-q:storybook-build --output-style=stream-without-prefixes",
    "build:storybook:css": "nx run bee-q:storybook-build-css --output-style=stream-without-prefixes",
    "test": "run-s test:*",
    "test:spec": "nx affected --target=test --parallel=1 --exclude=workspace --output-style=stream-without-prefixes",
    "test:e2e": "nx affected --target=e2e --parallel=1 --exclude=workspace --output-style=stream-without-prefixes",
    "clean": "run-s clean:build clean:modules",
    "clean:build": "rimraf ./dist/**",
    "clean:modules": "rimraf node_modules .cache",
    "pre-commit": "lint-staged --verbose",
    "prepare": "husky install",
    "postinstall": "node node_modules/puppeteer/install.js && ngcc --properties es2020 browser module main",
    "g": "plop --cwd tools/generators/plop"
  },
  "dependencies": {
    "@angular/animations": "14.2.12",
    "@angular/common": "14.2.12",
    "@angular/compiler": "14.2.12",
    "@angular/core": "14.2.12",
    "@angular/forms": "14.2.12",
    "@angular/platform-browser": "14.2.12",
    "@angular/platform-browser-dynamic": "14.2.12",
    "@angular/router": "14.2.12",
    "@floating-ui/dom": "1.2.6",
    "@swc/helpers": "0.5.0",
    "core-js": "3.30.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "regenerator-runtime": "^0.13.11",
    "rxjs": "7.8.0",
    "tslib": "2.5.0",
    "zone.js": "0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.2.10",
    "@angular/cli": "14.2.10",
    "@angular/compiler-cli": "14.2.12",
    "@angular/language-service": "14.2.12",
    "@commitlint/cli": "17.6.1",
    "@commitlint/config-angular": "17.6.1",
    "@commitlint/config-conventional": "17.6.1",
    "@jscutlery/semver": "2.29.3",
    "@nrwl/angular": "14.8.8",
    "@nrwl/cli": "14.8.8",
    "@nrwl/devkit": "14.8.8",
    "@nrwl/eslint-plugin-nx": "14.8.8",
    "@nrwl/jest": "14.8.8",
    "@nrwl/js": "14.8.8",
    "@nrwl/nx-cloud": "^15.1.0",
    "@nrwl/nx-plugin": "14.8.8",
    "@nrwl/react": "14.8.8",
    "@nrwl/storybook": "14.8.8",
    "@nrwl/web": "14.8.8",
    "@nrwl/workspace": "14.8.8",
    "@nxext/stencil": "14.0.6",
    "@stencil/angular-output-target": "0.6.0",
    "@stencil/core": "3.2.1",
    "@stencil/react-output-target": "0.5.0",
    "@stencil/sass": "3.0.1",
    "@storybook/addon-a11y": "6.5.16",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-docs": "6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-links": "6.5.16",
    "@storybook/builder-webpack5": "6.5.16",
    "@storybook/manager-webpack5": "6.5.16",
    "@storybook/mdx2-csf": "1.0.0",
    "@storybook/web-components": "6.5.16",
    "@swc-node/register": "1.6.4",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.51",
    "@tailwindcss/line-clamp": "0.4.4",
    "@testing-library/react": "13.4.0",
    "@types/jest": "27.5.1",
    "@types/mdx": "2.0.4",
    "@types/node": "18.15.11",
    "@types/puppeteer": "7.0.4",
    "@types/react": "18.0.35",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "@typescript-eslint/parser": "5.58.0",
    "@whitespace/storybook-addon-html": "5.1.4",
    "autoprefixer": "10.4.13",
    "chromatic": "6.17.3",
    "copy-webpack-plugin": "11.0.0",
    "copyfiles": "2.4.1",
    "decompress": "4.2.1",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "fs-extra": "11.1.0",
    "husky": "8.0.3",
    "jest": "27.5.1",
    "jest-environment-jsdom": "27.5.1",
    "jsonc-eslint-parser": "2.1.0",
    "lint-staged": "13.2.1",
    "lit-html": "2.7.2",
    "nanospinner": "1.1.0",
    "ng-packagr": "14.2.0",
    "ngx-deploy-npm": "^5.2.0",
    "npm-run-all": "4.1.5",
    "nx": "14.8.6",
    "nx-stylelint": "14.0.2",
    "plop": "3.1.2",
    "postcss": "8.4.22",
    "postcss-import": "^15.1.0",
    "postcss-preset-env": "^8.3.2",
    "postcss-url": "10.1.3",
    "prettier": "2.8.5",
    "prettier-plugin-tailwindcss": "0.2.7",
    "puppeteer": "19.9.0",
    "react-test-renderer": "18.2.0",
    "rimraf": "5.0.0",
    "stencil-tailwind-plugin": "1.7.0",
    "stylelint": "15.5.0",
    "stylelint-config-sass-guidelines": "10.0.0",
    "stylelint-config-standard": "32.0.0",
    "tailwindcss": "3.3.1",
    "terser-webpack-plugin": "5.3.6",
    "ts-jest": "27.1.5",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  },
  "overrides": {
    "@jest/environment": "$jest",
    "@mdx-js/react": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "@nrwl/devkit": "$@nrwl/devkit",
    "@nrwl/jest": {
      "jest": "$jest"
    },
    "@nxext/stencil": {
      "@stencil/core": "$@stencil/core"
    },
    "@storybook/addon-docs": {
      "@storybook/mdx2-csf": "$@storybook/mdx2-csf"
    },
    "nx-stylelint": {
      "stylelint": "$stylelint"
    },
    "stylelint-config-sass-guidelines": {
      "stylelint": "$stylelint"
    },
    "jest": "$jest",
    "jest-config": "$jest"
  },
  "volta": {
    "node": "18.15.0",
    "npm": "9.5.0"
  }
}
