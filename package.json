      "@custom-elements-manifest/analyzer": "$@custom-elements-manifest/analyzer",
      "eslint": "$eslint",
      "eslint-config-prettier": "$eslint-config-prettier",
      "react": "$react",
      "react-dom": "$react-dom",
      "vite": "$vite"
    },
    "patchedDependencies": {
      "@nxext/stencil@20.0.6": "patches/@nxext__stencil@20.0.6.patch"
    }
  },
  "packageManager": "pnpm@10.6.2",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  },
  "volta": {
    "node": "22.14.0",
    "pnpm": "10.6.2"
  }
