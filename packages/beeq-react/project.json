{
  "name": "beeq-react",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/beeq-react/src",
  "projectType": "library",
  "tags": ["wrapper", "react", "publishable"],
  "implicitDependencies": ["beeq"],
  "targets": {
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/beeq-react",
        "tsConfig": "packages/beeq-react/tsconfig.lib.json",
        "project": "packages/beeq-react/package.json",
        "entryFile": "packages/beeq-react/src/index.ts",
        "external": ["react", "react-dom", "react/jsx-runtime"],
        "rollupConfig": "@nx/react/plugins/bundle-rollup",
        "compiler": "babel",
        "assets": [
          {
            "glob": "packages/beeq-react/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "publish": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "access": "public",
        "noBuild": true,
        "tag": "latest"
      }
    },
    "publish-beta": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "access": "public",
        "tag": "beta"
      }
    }
  }
}
