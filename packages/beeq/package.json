{
  "name": "@beeq/core",
  "version": "1.3.3",
  "license": "Apache-2.0",
  "description": "BEEQ, a Design System components ruled by Endavan developers :)",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "collection": "./dist/collection/collection-manifest.json",
  "collection:main": "./dist/collection/index.js",
  "unpkg": "./dist/beeq/beeq.esm.js",
  "exports": {
    ".": {
      "import": "./dist/beeq/beeq.esm.js",
      "require": "./dist/cjs/beeq.cjs.js"
    },
    "./loader": {
      "import": "./dist/loader/index.js",
      "require": "./dist/loader/index.cjs",
      "types": "./dist/loader/index.d.ts"
    }
  },
  "files": [
    "dist/"
  ],
  "dependencies": {
    "@floating-ui/core": "^1.6.2",
    "@floating-ui/dom": "^1.6.5",
    "@stencil/core": "^4.18.1",
    "cally": "0.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Endava/BEEQ"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
